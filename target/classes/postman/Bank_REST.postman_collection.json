		{
			"name": "User One register",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Статус ответа 201\", function () { pm.response.to.have.status(201); });\r",
							"pm.test(\"Ответ содержит id пользователя\", function () { var jsonData = pm.response.json(); pm.expect(jsonData).to.have.property(\"id\"); });\r",
							"pm.test(\"Email совпадает\", function () { var jsonData = pm.response.json(); pm.expect(jsonData.email).to.eql(\"user1@example.com\"); });"
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [],
				"body": {
					"mode": "raw",
